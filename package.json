{
  "name": "brook-logger",
  "version": "1.0.0",
  "description": "Basic Logger",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "npx eslint src",
    "check-format": "prettier --check \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm test && npm run lint && npm run format && npm run build"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jkan1/brook-logger.git"
  },
  "author": "Jkan1",
  "keywords": [
    "Logger"
  ],
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jkan1/brook-logger/issues"
  },
  "homepage": "https://github.com/Jkan1/brook-logger#readme",
  "devDependencies": {
    "eslint": "^8.8.0",
    "jest": "^27.3.1",
    "prettier": "^2.4.1",
    "typescript": "^4.4.4"
  }
}
